@model IEnumerable<MyApplication.Models.QakhPage>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Qakh Page</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-lg-12">
        <p>
            <a asp-action="Create">Create New</a>
        </p>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.SecondAdditional)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ImagePath)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ImageText)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Title)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SubTitle)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Duration)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Code)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Price)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AboutTourText)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AboutTourNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourImage)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourInformation)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourVideo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourCheckIn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourText)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourTime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TourAdditional)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Number)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SecondNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SiteAddress)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Address)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.SecondAdditional)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ImagePath)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ImageText)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Title)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SubTitle)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Duration)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Code)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Price)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AboutTourText)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AboutTourNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourImage)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourInformation)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourVideo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourCheckIn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourText)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TourAdditional)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Number)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SecondNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SiteAddress)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Address)
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
@model MyApplication.Models.HotelListDescription

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Create Hotel List Description</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="HotelLink" class="control-label"></label>
                <input asp-for="HotelLink" class="form-control" />
                <span asp-validation-for="HotelLink" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ListHotel" class="control-label"></label>
                <input asp-for="ListHotel" class="form-control" />
                <span asp-validation-for="ListHotel" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

